{
  "name": "resfetch",
  "type": "module",
  "version": "1.0.0",
  "packageManager": "bun@1.3.4",
  "description": "A tiny, type-safe fetch wrapper with Standard Schema validation, Result pattern, and zero dependencies",
  "license": "MIT",
  "keywords": [
    "fetch",
    "http",
    "request",
    "api",
    "client",
    "typescript",
    "type-safe",
    "schema",
    "validation",
    "zod",
    "valibot",
    "arktype",
    "standard-schema",
    "result",
    "error-handling"
  ],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs"
  },
  "main": "dist/index.mjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "lint": "eslint .",
    "release": "bumpp",
    "typecheck": "tsc --noEmit",
    "test": "vitest run --coverage",
    "test:api": "bun test tests/real-api.test",
    "preinstall": "npx -y only-allow bun",
    "postinstall": "simple-git-hooks"
  },
  "dependencies": {},
  "devDependencies": {
    "@antfu/eslint-config": "^6.5.1",
    "@standard-schema/spec": "^1.0.0",
    "@vitest/coverage-v8": "4.0.15",
    "bumpp": "^10.2.3",
    "lint-staged": "^16.2.7",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "vitest": "4.0.15",
    "zod": "^4.1.13"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
